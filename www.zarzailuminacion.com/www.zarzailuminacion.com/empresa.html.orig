        function stopBanner() {

          if (bannerRunning){
                clearTimeout(timerID)
          }
          bannerRunning = false
        }
       
        function startBanner() {
          stopBanner()
          showBanner()
        }

        function showBanner() {

          var text = ar[currentMessage]//currentMessage°¡ 0ÀÌ¸é text´Â "Welcome!"
          if (offset < text.length) {
                if (text.charAt(offset) == " "){
                        offset++
                }

                //substring()ÇÔ¼ö´Â text°¡  Welcome!ÀÌ¸é text.substring(0,1)´Â W                                      
                var partialMessage = text.substring(0, offset + 1) 
                window.status = partialMessage
                offset++ 
                timerID = setTimeout("showBanner()", speed)//showBanner()¸¦ speed(0.05ÃÊ)µÚ¿¡ È£Ãâ
                bannerRunning = true
          }

          else {
                offset = 0
                currentMessage++

                if (currentMessage == ar.length){
                    currentMessage = 0
                }

                timerID = setTimeout("showBanner()", pause)
                bannerRunning = true
          }
        }

