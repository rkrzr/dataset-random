.excerpt {overflow: hidden;}
.excerpt a img, .excerpt img {float: left;margin: 10px 10px 10px 0;}
.excerpt h1, .excerpt h2, .excerpt h3, .excerpt h4 {clear: none;}

li.comment {margin-top: 10px;}
