body	{
	margin: 0em;
	font-family: Arial, Verdana;
	color: #000000;
	font-size: 12px;
	text-decoration: none;
	font-weight: normal;
	background-image: url(../images/bg_left.gif);
	background-repeat: repeat-y
}

td	{
	font-family: Arial, Verdana;
	color: #000000;
	font-size: 12px;
	text-decoration: none;
	font-weight: normal;
	line-height: 16px
	}


.bg_black	{
	background-color: #000000
}
.bg_white	{
	background-color: #FFFFFF
}
.bg_grey1	{
	background-color: #A7B0B5
}
.bg_grey2	{
	background-color: #C1CBD1
}
.bg_grey3	{
	background-color: #768C9A
}
.bg_blue1	{
	background-color: #6AABCD
}
.bg_blue2	{
	background-color: #0084C8
}
.bg_blue3	{
	background-color: #6B8392
}
.bg_shell	{
	background-color: #EFF4F5
}

li {
  margin-left: 10px;
  color: #0084C8; 
  list-style-type: square;
  margin-top: 6px; 
  margin-bottom: 6px;
}
.listheight { 

  }

hr {
  height: 1px; 
  color: #666666; 
  line noshade
  }
  
.hrfade {
  height: 1px; 
  color: #B5B5B5; 
  line noshade
  }

h1	{
	font-family: Arial, Helvetica, sans-serif;
	color: #000000;
	font-size: 16px;
	text-decoration: none;
	font-weight: bold
}
h2	{
	font-family: Arial, Helvetica, sans-serif;
	color: #000000;
	font-size: 13px;
	text-decoration: none;
	font-weight: bold
}



h3 {
	font-size: 17px;
	font-weight: bold;
	color:#0084C8;
	margin:0 0 4px 0;
}
h4 {
	font-size: 14px;
	font-weight: bold;
	color:#444;
	margin:0 0 10px 0;
}

a       {
	font-family: Arial, Helvetica, sans-serif;
	color: #0084C8;
	font-size: 12px;
	text-decoration: underline;
	font-weight: bold
}

a:hover {
	font-family: Arial, Helvetica, sans-serif;
	color: #0084C8;
	font-size: 12px;
	text-decoration: underline;
	font-weight: bold
}

.toplink       {
	font-family: Arial, Helvetica, sans-serif;
	color: #0084C8;
	font-size: 11px;
	text-decoration: none;
	font-weight: normal
}

.toplink:hover {
	font-family: Arial, Helvetica, sans-serif;
	color: #0084C8;
	font-size: 11px;
	text-decoration: none;
	font-weight: normal
}

.sitemap       {
	font-family: Arial, Helvetica, sans-serif;
	color: #768C9A;
	font-size: 4px;
	text-decoration: none;
	font-weight: normal
}
.sitemap:hover       {
	font-family: Arial, Helvetica, sans-serif;
	color: #768C9A;
	font-size: 4px;
	text-decoration: none;
	font-weight: normal
}

.search       {
	font-family: Arial, Helvetica, sans-serif;
	color: #FFFFFF;
	font-size: 11px;
	text-decoration: none;
	font-weight: normal
}

.toplinks       {
	font-family: Arial, Helvetica, sans-serif;
	color: #333333;
	font-size: 11px;
	text-decoration: none;
	font-weight: normal
}

.toplinks:hover {
	font-family: Arial, Helvetica, sans-serif;
	color: #333333;
	font-size: 11px;
	text-decoration: none;
	font-weight: normal
}

.text       {
	font-family: Arial, Verdana;
	color: #000000;
	font-size: 12px;
	text-decoration: none;
	font-weight: normal;
	line-height: 16px;
	margin-top: 6px; 
 	margin-bottom: 6px;
	list-style-position: outside
}
.smalltext       {
	font-family: Arial, Helvetica, sans-serif;
	color: #000000;
	font-size: 10px;
	line-height: 10px; 
	text-decoration: none;
	font-weight: normal
}
.smalltext:hover       {
	font-family: Arial, Helvetica, sans-serif;
	color: #000000;
	font-size: 10px;
	line-height: 10px; 
	text-decoration: none;
	font-weight: normal
}

.shell {  
	border: 1px #6E8C9E solid;
	background: #EFF4F5;
	font-family: Arial, Verdana;
	color: #000000;
	font-size: 12px;
	text-decoration: none;
	font-weight: normal;
	line-height: 16px;
	margin-top: 6px; 
 	margin-bottom: 6px;
	list-style-position: outside
}

.formbutton {
	background: #D4D0C8;
	font-family: Arial; 
	font-size: 11px; 
	color: #000000;
	height: 21px;
} 
