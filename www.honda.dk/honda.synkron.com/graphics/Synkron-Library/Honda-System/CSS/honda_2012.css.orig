/* typography
--------------------------------------- */
body {font: 14px/1.5 Helvetica, Arial, Verdana, sans-serif; color: #4b4b4b;}
h1, h2, h3, h4, h5, h6, .narrowcol th {font-family: "HelveticaNeue-Light", Arial, Helvetica, Verdana, sans-serif; font-weight: normal; margin-bottom: .4em; letter-spacing: 0; line-height: 1.1em; display: block; margin-top: .7em; text-rendering: optimizeLegibility;}
h1 {font-size: 3em; margin: 0 0 .1em; padding: 1em 0 0;} h2, .narrowcol th {font-size: 1.7em;} h3 {font-size: 1.5em;} h4 {font-size: 1.2em; } h5 {font-size: 1em;} h6 {font-size: .8em;}
h1 a, h2 a, h3 a, h4 a, h5 a, h6 a {color: #4b4b4b;text-decoration: none;border: none;}
h1 a:hover, h2 a:hover, h3 a:hover, h4 a:hover, h5 a:hover, h6 a:hover {border-bottom: 1px dotted;}
p {margin-bottom: 1.1em;}
a {text-decoration: none; /*border-bottom: 1px dotted;*/}
a:hover {/*border-bottom: 1px solid;*/}
.discreet, .discreet a {color: #999;}
blockquote {font-style: italic;	margin-left: 2em;}
strong {font-weight: bold;}
.leadin p,
p.ingress {font: 1.15em/1.5 helvetica;}
.page-title {font-size: .6em; font-weight: 500; letter-spacing: 0.1em; line-height: 2.6em; margin: 0 0 2.6em; text-transform: uppercase;}


/* layout
----------------------------------------- */
body {background-color: #f9f7ef;}
a {color: #cc0000; border: none;}
a:hover {color: #333;}
#wrapper_outer {
	margin: 0 auto;
	width: 960px;
	padding-left: 0;
/*	outline: 1px solid green;*/
	}
#wrapper,
#wrapper_models,
#wrapper_firstpage,
.inner {
	width: 960px;
	margin: 0 auto;
	}
#head {
	padding-top: 3.2em;
	display: block;
	position: relative;
	}
#contentarea {
	float: right;
	width: 653px;
	background-color: #fff; 
	padding: 3em;
	margin-bottom: 4em;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	-o-border-radius: 2px;
	border-radius: 2px;
	}
#contentarea,
#leftnav,
#bigcar a.shadow,
.puff img {
	-moz-box-shadow: 0 1px 6px rgba(0, 0, 0, .15);
	-webkit-box-shadow: 0 1px 6px rgba(0, 0, 0, .15);
	-o-box-shadow: 0 1px 6px rgba(0, 0, 0, .15);
	box-shadow: 0 1px 6px rgba(0, 0, 0, .15);
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	-o-border-radius: 2px;
	border-radius: 2px;
	overflow: hidden;
	}

.firstpagebanner {float: left; width: 217px;}
.firstpage_bottomarea {}
	
