/* firstpage
------------------------------------------*/
.firspage_bannerarea {
	float: left;
	background-color: #fff;
	width: 326px;
	}
.firspage_bannerarea p {
	padding: 15px;
	}
#smallcars {
	width: 360px;
	float: left;
	margin: 5px 0 20px 5px;
	}
#smallcars a {
	border: none;
	}
#bigcar {
	position: relative;
	margin: 2em 0 10px;
	}
#bigcar a.shadow {
	border: 2px solid #fff;
	box-shadow: 0 1px 3px rgba(0, 0, 0, .35);
}
#bigcar a img {
	float: left;
	padding: 0;
	}
#bigcar .content {
	background: url('/graphics/Synkron-Library/Honda-System/Images/bg_transparent_50.png');
	background: rgba(0, 0, 0, .5);
	width: 45%;
	position: absolute;
	top: 30px;
	right: 30px;
	padding: 20px;
	color: #fff;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	-o-border-radius: 2px;
	border-radius: 2px;
	}
#bigcar .content a {color: #fff; border: none;}
#bigcar .content a:hover {color: #c33237;}
#bigcar h1 {padding: .1em 0 0;}
#bigcar p {margin: 0;}
#bigcar a {
	color: #cc0000; 
	text-decoration: none;
	border: 1px solid #bbb;
/*	outline: 1px solid orange;*/
	display: block;
	}
#bigcar a:hover {text-decoration: none;}

.puff {
	position: relative;
	background-color: #fff;
	width: 309px;
	float: left;
	margin: 0 10px 10px 0;
	border-radius: 2px;
	box-shadow: 0 1px 3px rgba(0, 0, 0, .35);
	border: 2px solid #fff;
	}
.puff h3 {
	position: absolute;
	color: #fff;
	top: 3px;
	left: 20px;
	margin-right: 20%;
	text-shadow: 0 0 1px rgba(0, 0, 0, .6);
}
.puff h3.dark {color: #c33237;}
.puff h3.small {width: 60%;}
.last {margin-right: 0 !important;}

.puff.x2 {
	width: 471px;
	height: 180px;
	margin-right: 10px;
	background-color: #EAEBE5;
}

