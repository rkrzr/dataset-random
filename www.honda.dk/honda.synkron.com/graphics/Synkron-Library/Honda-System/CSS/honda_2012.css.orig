/* nav
------------------------------------------*/
.nav ul {
	list-style-type: none;
	background: #edecec;
	background: -webkit-gradient(linear, left top, left bottom, from(#fbfbfb), to(#e9e9e9));
	background: -moz-linear-gradient(top, #fbfbfb, #e9e9e9);
	background: -o-linear-gradient(top, #fbfbfb, #e9e9e9);
	background: linear-gradient(top, #fbfbfb, #e9e9e9);
/*	background-color: #fff;*/
	}
.nav ul li {
	display: inline;
	float: left;
	}
.nav ul li.last {margin-right: 0;}
.nav ul li a {
	display: inline-block;
	width: 105px;
	color: #494f50;
	vertical-align: bottom;
	text-decoration: none;
	}
.pagelang_fi .nav ul li#topnav_5 a {width: 140px;}
	
.nav ul li a:hover,
.nav ul li a.selected {
	background: filter:  progid:DXImageTransform.Microsoft.gradient(startColorStr='#ac0000', EndColorStr='#cc0000'); /* IE6,IE7 */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#ac0000', EndColorStr='#cc0000')"; /* IE8 */
	background: -webkit-gradient(linear, left top, left bottom, from(#ac0000), to(#cc0000));
	background: -moz-linear-gradient(top, #ac0000, #cc0000);
	background: -o-linear-gradient(top, #ac0000, #cc0000);
	background: linear-gradient(top, #ac0000, #cc0000);;;
	color: #fff;
	border: none;
	text-shadow: none;
	border-right: 1px solid #ddd;
	border-right: 1px solid rgba(0, 0, 0, .1);
	}


