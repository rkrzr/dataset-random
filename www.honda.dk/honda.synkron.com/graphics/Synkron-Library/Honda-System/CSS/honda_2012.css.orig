/* clearing
----------------------------------------- */
.group:after,
#masthead:after,
.inner:after,
.topnav ul:after,
#maincontent:after,
.midnav ul:after,
/*#footer:after,*/
#footer_models:after,
#footer_colophon:after {content: "."; display: block; height: 0; clear: both; visibility: hidden;}

.rightjustified {text-align: right;}
.clear,
.visualClear {display: block; clear: both;}
.content_leftcolumn {
	width: 328px;
	float: left;
/*	outline: 1px solid orange;*/
	}
.pointer {cursor: pointer;}
.movie {background-color: #fff;}
.arrow { /* för att få till en schysst bildväxlare */
	width: 90px;
	margin-right: 0;
	display: block;
	text-align: right;
	vertical-align: bottom;
	float: right;
	padding: 6px 3px 0;
	}
.arrow img {display: inline; margin-left: 5px; opacity: .7;}
.arrow img:hover {opacity: 1;}

