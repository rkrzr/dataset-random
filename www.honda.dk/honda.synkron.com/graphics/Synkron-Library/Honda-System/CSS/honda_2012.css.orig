.file_link a {background: url(/graphics/Synkron-Library/Honda-System/Images/icon_pdf.gif) no-repeat;}
.external_link a {background: url(/graphics/Synkron-Library/Honda-System/Images/icon_externallink.gif) no-repeat;}
.media_link_movie a {background: url(/graphics/Synkron-Library/Honda-System/Images/icon_movie.gif) no-repeat;}
.media_link_sound a {background: url(/graphics/Synkron-Library/Honda-System/Images/icon_audio.gif) no-repeat;}
.media_link_wallpaper a {background: url(/graphics/Synkron-Library/Honda-System/Images/icon_image.gif) no-repeat;}

.file_link a,
.external_link a,
.media_link_movie a,
.media_link_sound a,
.media_link_wallpaper a  {background: none;}




/* searchbox
------------------------------------------*/
.searcharea {
	position: absolute;
	top: 15px;
	width: 270px;
	right: 0;
	margin: 0;
	padding: 0;
	}	
.pagelang_fi .searcharea,
.pagelang_dk .searcharea{width: 270px;}
.searcharea img {display: none;}
.searcharea .norm {margin: 0; padding: 0;}
.searcharea .norm input {
	border: 1px solid #ccc;
	padding: .4em .6em;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	-o-border-radius: 3px;
	border-radius: 3px;
	margin-right: 20px;
	}
.searcharea table {
	margin: 0;
	padding: 0;
	float: left;
/*	width: 260px;*/
	border: none;
/*	outline: 1px solid orange;*/
	
	}
.searcharea table table {
	margin: 0 8px 0 0;
	padding: 0;
	width: 180px;
	border: none;
	}
.searcharea table td {
	margin: 0;
	padding: 0;
	padding-right: 2px;
	}
.swformsingle {
	margin: 0;
	padding: 0;
	width: 99%;
	}
.swformbutton,
.button {
	float: left;
	border: none;
	cursor: pointer;
	padding: .43em .8em;
	margin-left: 10px;
	color: #fff;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	-o-border-radius: 3px;
	border-radius: 3px;
/*	background: url(/graphics/Synkron-Library/Honda-System/Images/icon_linkarrow.gif) 0 8px no-repeat;*/
	background: filter:  progid:DXImageTransform.Microsoft.gradient(startColorStr='#c33237', EndColorStr='#a1292d'); /* IE6,IE7 */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#c33237', EndColorStr='#a1292d')"; /* IE8 */
	background: -webkit-gradient(linear, left top, left bottom, from(#c33237), to(#a1292d));
	background: -moz-linear-gradient(top, #c33237, #a1292d);
	background: -o-linear-gradient(top, #c33237, #a1292d);
	background: linear-gradient(top, #c33237, #a1292d);;
	
}


