@import url("css/menu.css");

/* CSS Document */
body {background:#FFFFFF repeat-x top left; height:100%; margin:0px; padding:0px; }

/*-------- Default Style --------*/
th,td,div{color:#000; font-family:Verdana, Arial, Helvetica, sans-serif; font-weight:normal; }
a,a:link,a:visited,a:active{color:#FF7A03; text-decoration:none; font-family:Verdana, Arial, Helvetica, sans-serif; font-weight:normal; }
a:hover{color:#0000DD; text-decoration:none; }

/*-------- design style --------*/
.template_style{width:960px; margin:auto;}

.lang_pad{ position: relative;  top: 0px; left: 0px;}
.logo_pad{ position: relative;  top: 0px; right: 0px; margin: 0 0px 16px 0px; padding:0 ;}
.logo_pad img { float: right; }

/*SC   */
.col {width: 48%;}
.left {float: left;}
.right {float: right;}
.c_content div.c_contentpane, .c_footer { 
  padding: 0px  0px  0px  0px !important; 
  margin: 0px  0px  0px  0px !important;
}

/*SC  content styles */
div.error-msg {
display: block;
border-top: 1px solid red;
padding: 20px 0 0 0;
margin: 20px 0 20px 0;

}

div.error-msg img {
float: left;
margin: 0 20px 20px 0px;
}
div.error-msg span {
float: left;
}
.img-block img {
  float: left;
  margin: 0 0 10px -130px;  
}
.img-block {
  padding: 0 0 0 130px;
  min-height: 130px;
}
.img-block {
  clear: both;
}
td, th {
  vertical-align: top;
  text-align: left;
}
.clear-float {
  clear: both;
}

.c_head {
  padding: 5px 5px 5px 0px !important; 
  margin: 10px 0 5px 0 !important;

}
.c_head.title_grey .Head{

}
h2, h2 span.Head {
  font-size:14px;
  font-weight:bold;
  color: #5b7067;
  text-align: left;
}
h3 {
  padding: 5px !important; 
  margin: 10px 0 5px 0 !important;
  background-color: #010B6D;
  color: #fff;
  font-size:14px;
  font-weight:bold;
}
h4, h4 a {
  color: #010B6D;
  font-weight: bold;
}
/* events */
.ListHeader {
	background-image: none !important;
}
div#map_border {
  background-color: #d9e3df;
  float: right;
  padding: 22px;
}
div#map_canvas {
  border: 3px solid #fff !important;
}
table#tbl-editevent td, table#tbl-editevent th {
  vertical-align: top;
  text-align: left;
  padding: 0 20px 10px 0;
}

.menu_style{float:left; padding: 0px; margin: 0px;}
.menu_bg {padding:0 0 0 0px;   height: 32px; margin: 0 19px 0 0px; }
.main_dnnmenu_rootitem  {	background-color: #010B6D; margin-right: 2px !important; padding: 5px !important;}
.main_dnnmenu_rootitem_selected { background-image: none; background-color: #F08801; margin-right:2px; padding:5px; }
.main_dnnmenu_rootitem_hover {  background-image: none; background-color: #F08801; }
.menu_left{padding:0px; margin:0px; border-bottom: 6px solid #F08801;}


.search_style{float:right; padding:0px 0px 0px 0px; height: 19px;}
.search_bg{padding-right:2px;}
.search_bg Input{color: #43494D;}


.bread_bg{padding:0px; margin: 0 19px 0 0px; background:url(images/bread_bg_gray.png) repeat-x top left; height:34px;}
.bread_left{padding:0px; margin:0px; background:url(images/bread_left.png) no-repeat top left;}
.bread_right{padding:0px;  background:url(images/bread_right.png) no-repeat top right;}

#login_style{ float:right; padding:8px 17px 0px 10px; }
#bread_style{ float:left; padding:8px 0px 0px 0px; color:#000000; font-size:13px; }

.center_bg{padding:0px; margin:0px;}
.left_bg{padding:0px; margin:0px; }
.right_bg{padding:0px; margin:0px; }
.content_pad{padding:0px 0px 10px 0px; margin: 0 0px 0 0px;}

.bot_left{padding:5px 0px 0px 0px; margin:0px; background:url(images/footer_bg.png) repeat-x top left;}
.bot_right{padding:0px; margin:0px;  padding:0px 39px 0px 0px;}
.bot_bg{padding: 0px; margin:0px; height: 41px;}

.bot_pad{margin-bottom:20px;padding:0 0px 0 0px;}
#terms_style{float:left;}
#copy_style{float:right;}

.clear_float{clear:both;}

/*--------- pane style ----------*/
.ContentPane, .TopPane, .BottomPane{ padding:0px; margin:0px; }
.LeftPane{ padding:0px 10px 0px 0px; width:auto; margin:0px; }
.RightPane{ padding:0px 0px 0px 10px; width:auto; margin:0px; }

/*------------ Breadcrumb Style --------------*/
.Breadcrumb,a.Breadcrumb:link,a.Breadcrumb:active,a.Breadcrumb:visited{ color:#4F839F; font-size:13px; }
a.Breadcrumb:hover{color:#0000DD;}

/*-------------- Link Style --------------*/
.links {text-align:center;}
.links,a.links:link,a.links:active,a.links:visited{ font-weight:bold; color:#657970; font-size:11px; text-transform:uppercase; }
a.links:hover{color:#0000DD;}

/*-------------- User Style --------------*/
.user,a.user:link,a.user:active,a.user:visited { color:#4F839F; font-size:12px;}
a.user:hover{color:#0000DD;}

/*-------------- Footer Style --------------*/
.footer,a.footer:link,a.footer:active,a.footer:visited { color:#657970; font-size:12px;}
a.footer:hover{color:#0000DD;}

/*-------------- Search SkinObject Styles --------------*/
div.SearchContainer{position:relative; white-space: nowrap;}
div.SearchBorder{width: 200px; height: 20px; border: solid 1px #e0e0e0; position:absolute; right:35px; background-color: White; white-space: nowrap; margin-top: 2px;}
div.SearchContainer a{padding: 0px 5px; position:relative; display: block;}
div.SearchIcon{float: left; width: 31px; height: 20px; cursor: pointer; z-index: 11; background: no-repeat 2px 2px; text-align:right;}
div.SearchIcon img{margin-top: 3px;}

input.SearchTextBox{float: left; z-index: 10; border: 0px; width:150px; height:18px; margin-left:3px;}

#SearchChoices{clear: both; display: none; border: solid 1px #FFFFFF; z-index: 2000; cursor: pointer; margin: 0; padding: 0; text-align:left; font-size:12px; background-color: #FFFFFF; position:absolute; top: 22px; left: 0px;}
#SearchChoices li{cursor: pointer; margin: 0; padding: 2px 2px 0 2px; padding-left: 25px; height: 18px; text-align:left; background-color: #FFFFFF; background-repeat: no-repeat; background-position: 2px center; list-style: none; list-style-image: none; border: none; display:block;}
#SearchChoices li.searchHilite{background-color: #0000DD;	color:#FFFFFF; border: none;}
#SearchChoices li.searchDefault{background-color: #FFFFFF; border: none;}

.hidden{color:white!important}

.colorPickerContainer { border: solid 1px #000000; display:block; clear:both; float:left; width:400px; height:200px}



















